@model IEnumerable<Category>;
@{
    ViewData["Title"] = "Index";
}

<h1>Index Category</h1>

<table class="table table-bordered table-stripped " style="width: 100%">

    <ul> 

        @foreach(var obj in Model)
        {
            <li> 
                <h3>@obj.Name</h3>
                <p>@obj.DisplayOrder</p>
            </li>
        }


    </ul>  

</table>
